{
  "name": "template-vitejs-vue3-tailwindcss-fontawesome",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.14.2"
  },
  "scripts": {
    "prepare-env": "node --experimental-fetch --loader ts-node/esm ./prepare-env.ts",
    "preinstall": "cd api && npm install",
    "dev": "npm run prepare-env && vite",
    "convert-typo": "node --loader ts-node/esm ./typo/generate-webfonts.ts",
    "build": "npm run prepare-env && vite build && gulp licenses",
    "preview": "vite preview",
    "start": "npx @azure/static-web-apps-cli start https://localhost:5173 --swa-config-location ./ -s -e localhost.lesmuids.windows-ext.pem -k localhost.lesmuids.windows-ext.key -p 8788 --api-location ./api -t 120 --run 'npm run dev'",
    "prestart": "cd api && npm run watch &"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.22.4",
    "@azure/cosmos": "^3.17.1",
    "@azure/functions": "^3.2.0",
    "@pqina/flip": "^1.7.7",
    "@vueuse/core": "^9.3.0",
    "jose": "^4.10.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.0.2",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/fontmin": "^0.9.0",
    "@types/glob": "^8.0.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "autoprefixer": "^10.4.12",
    "fontmin": "^0.9.9",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "^1.0.9",
    "gulp-replace": "^1.1.3",
    "npm-check-updates": "^16.3.7",
    "postcss": "^8.4.17",
    "postcss-purgefonts": "^1.0.2",
    "sass": "^1.55.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  }
}
