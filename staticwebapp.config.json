{
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*.{png,jpg,gif,svg,webp,woff,woff2,css,ttf,js}","!*"
        ]
    }
}